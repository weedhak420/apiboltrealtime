# Rename this file to .env and fill in the real credentials before running the app.
# These values are required by the Bolt realtime API integration.

# OAuth bearer token used for authenticating requests to the Bolt API.
BOLT_BEARER_TOKEN=replace-with-bolt-bearer-token

# Device metadata used by the Bolt mobile API.
BOLT_DEVICE_ID=replace-with-device-id
BOLT_DEVICE_NAME=replace-with-device-name
BOLT_DEVICE_OS_VERSION=replace-with-device-os-version

# User identity parameters for the Bolt API session.
BOLT_USER_ID=replace-with-user-id
BOLT_DISTINCT_ID=replace-with-distinct-id
BOLT_RH_SESSION_ID=replace-with-rh-session-id

# Optional overrides with sensible defaults.
BOLT_CHANNEL=googleplay
BOLT_BRAND=bolt
BOLT_DEVICE_TYPE=android
BOLT_COUNTRY=th
BOLT_LANGUAGE=th
